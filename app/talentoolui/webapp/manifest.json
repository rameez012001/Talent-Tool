{
  "_version": "1.76.0",
  "sap.app": {
    "id": "talentoolui",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "f7886980-3416-4a7f-95ef-74e7a64b4616"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/asset/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "talentoolui.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "talentoolui.view",
        "async": true,
        "viewPath": "talentoolui.view"
      },
      "routes": [
        {
          "name": "RouteTalentTool",
          "pattern": ":?query:",
          "target": [
            "TargetTalentTool"
          ]
        },
        {
          "name": "RouteAssetObject",
          "pattern": "asset/{id}",
          "target": [
            "TargetAssetObject"
          ]
        },
        {
          "name": "RouteAssetMaintenanceObject",
          "pattern": "maintenance_request/{id}",
          "target": [
            "TargetAssetMaintenanceObject"
          ]
        },
        {
          "name": "RouteUserObject",
          "pattern": "user/{id}",
          "target": [
            "TargetUserObject"
          ]
        }
      ],
      "targets": {
        "TargetTalentTool": {
          "id": "TalentTool",
          "name": "TalentTool"
        },
        "TargetAssetObject": {
          "id": "AssetObject",
          "name": "AssetObject"
        },
        "TargetAssetMaintenanceObject": {
          "id": "AssetMaintenanceObject",
          "name": "AssetMaintenanceObject"
        },
        "TargetUserObject": {
          "id": "UserObject",
          "name": "UserObject"
        }
      }
    },
    "rootView": {
      "viewName": "talentoolui.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
